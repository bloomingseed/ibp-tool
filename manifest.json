{
    "manifest_version": 3,
    "name": "instant Blogger Posting Tool",
    "description": "A tool to auto make Blogger posts quick.",
    "version": "1.0.0",
    "background":{
        "service_worker": "background-wrapper.js"
    },
    "icons":{
        "16": "/images/logo_16.png",
        "128":"/images/logo_128.png"
    },
    "action":{
        "default_icon":{
            "16": "/images/logo_16.png",
            "128":"/images/logo_128.png"
        }
    },
    "options_page": "options.html",
    "permissions":[
        "storage",
        "identity"
    ],
    "oauth2": {
        "client_id": "548512329264-51k74inm1vefgtk2egatt4h01g9i2a5q.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/blogger"
        ]
    },
    "key": ""
}